{
	"rulesDirectory": ["tslint-plugin-prettier"],
	"extends": ["tslint-config-prettier"],
	"rules": {
		"prettier": [
			true,
			{
				"singleQuote": true,
				"semi": true,
				"proseWrap": "never",
				"printWidth": 135,
				"useTabs": true,
				"tabWidth": 4,
				"bracketSpacing": true
			}
		]
	}
}
